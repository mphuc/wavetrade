
<div id="exchange_option" class="notranslate">


<div class="header_exchange" >
   <div class="col-md-2 text-center">
      <a href="/account/dashboard">
         <img src="/img/logo-right.png" style="margin-top: -7px;" width="120" alt="logo" class="img-responsive logos">  
      </a>
   </div>
   <div class="col-md-2 text-center hidden-xs hidden-sm">
      
      <div ng-controller="CtrCounDown" class="pull-right" style="margin-top: -1px; ">
      <!-- <p style="color: #1e90ff; float: left; font-size: 16px; margin-top: 4px; margin-right: 20px;">{[{date_now}]}</p> -->
         <div class="time-to-bet {[{type}]}" style="float: left; margin-left: 20px;">
            <span id="time-to-bet-text-1" class="green-text"> </span>
            <div id="time-to-bet-number-1" class="timer-counter green-timer">{[{second}]}</div>
         </div>
      </div>

   </div>
   <div class="col-md-4 col-xs-12 text-center ">
      <a id="is-play-sound" data-status="0" href="#" class="btn header-btn">
      <i  class="glyphicon glyphicon-volume-up"></i>
      </a>
      <a href="/account/history" class="btn header-btn">
      <i class="fa fa-history"></i>
      </a>
      <a href="/logout" class="btn header-btn">
      <i class="fa fa-power-off"></i>
      </a>
   </div>
   <div class="col-md-2 col-xs-6 text-center">
      <div class="CtrBalanceUser" ng-controller="CtrBalanceUser">
         <div  >
            <h4 class="text-center">
               <img src="/img/balance.png" class="hidden-xs" width="30"> <span class="balance">{[{balance | formatNumber}]} USD</span>
            </h4>
         </div>
      </div>
   </div>
   <div class="col-md-2 col-xs-6 text-right right_header_top ">
