<section class="account-list" style="margin-top: 30px;">
   <div class="">
      <div class="box">
         <div class="box-body">
            
            <section class="transfer">
               <div class="">
                  <div class="card">
                     <div class="card-header"><i class="fa fa-exchange m-r-5"></i> Member  list</div>
                     <div class="card-body">

                        <div class="table-responsive">
                           <div class="material-datatables" style="margin-top: 20px;">
                           <table  id="login_historyss" class="table table-striped table-no-bordered table-hover" style="width:100%;cellspacing:0">
                              <thead>
                                 <tr>
                                    <!-- <th>#</th> -->
                                    <th class="text-center">Email</th>
                                    <th class="text-center">Activated Date</th>
                                    <th class="text-center">Sponsor</th>
                                    <th class="text-center">Floor</th>
                                    <th class="text-center">Trade Amount</th>
                                    
                                    <th class="text-center">Balance</th>
                                    <th class="text-center">Direct Member</th>
                                    <th class="text-center">Transfer</th>
                                 </tr>
                              </thead>
                              
                              <tbody>
                                 {{#each child}}
                                    
                                    <tr>
                                       <!-- <td class="text-left">{{math1 @index}}</td> -->
                                       <td class="text-left">{{this.email}}</td>
                                       <td class="text-left">{{prettifyDate this.signupDate}}</td>
                                       <td class="text-left">{{this.sponsor}}</td>
                                       <td class="text-center">{{this.floor}} </td>
                                       <td class="text-right">{{this.betting}} </td>
                                       
                                       <td class="text-right">{{this.balance}} </td>
                                       <td class="text-right">{{this.count_f1}} </td>
                                       <td class="text-center">
                                          <span style="cursor: pointer;" class="transfer-id-button" data-email="{{this.email}}"><i class="fa fa-cloud-upload"></i></span>
                                       </td>
                                    </tr>
                                    
                                 {{/each}}
                              </tbody>
                           </table>
                           <p class="" style="margin-top: -38px; float: left;">Total Member: {{total_member}}. Total F1: {{total_f1}}. Total Trade Amount: {{formatusd total_betting}}</p>
                           </div>
                        </div>
                     </div>
                  </div>
               </div>
            </section>

            <div class="">

               <div class="" style="min-height: 500px;
               ">
                  <div class="card">
                     <input id="refuid" type="hidden" value="{{user._id}}" class="form-control"> 
                     <div id="TreeSystem" class="demo"></div>
                  </div>

               </div>
            </div>

         </div>
      </div>
   </div>
</section>
 