         <div class="content" style="margin-top: 40px;">
           
            <table  class="table_balance table table-striped table-no-bordered table-hover" style="width:100%;cellspacing:0" >
               <thead>
                  <tr> 
                     <th>Date</th>
                     <th>Amount Coin</th>
                     <th>Amount Payment</th>
                     
                     <th>Status</th>
                  </tr>
               </thead>
               <tbody>
                  {{#each order_history}} 
                     
                     <tr>
                        <td>{{prettifyDate this.date}}</td>
                        <td>{{satoshi this.amount_coin}} SFCC</td>
                        <td>{{satoshi this.amount_payment}} {{ this.method_payment}}</td>
                        {{#if_eq this.status '0'}}
                          <td><span class="label label-warning">Processing</span></td>

                        {{/if_eq}} 
                        {{#if_eq this.status '1'}}
                          <td><span class="label label-success">Finish</span></td>
                        {{/if_eq}} 
                        {{#if_eq this.status '3'}}
                          <td><span class="label label-danger">Cancel Order</span></td>
                        {{/if_eq}} 
                     </tr>
                    
                   {{/each}} 
               
               </tbody>
            </table>
         </div> 


         <div class="content" style="margin-top: 5px;">
           <h4 class="text-center">Get 5% Referal Bonus</h4>
            <table  class="table_balance table table-striped table-no-bordered table-hover" style="width:100%;cellspacing:0" >
               <thead>
                  <tr> 
                     <th>Date</th>
                     <th>Amount Coin</th>
                     <th>Details</th>
                     
                   
                  </tr>
               </thead>
               <tbody>
                  {{#each user.balance.coin_wallet.history}} 
                     {{#if_eq this.type 'refferalico'}}
                     <tr>
                        <td>{{prettifyDate this.date}}</td>
                        <td>{{this.amount}} SFCC</td>
                        <td>{{this.detail}} </td>
                        
                       
                     </tr>
                     {{/if_eq}} 
                   {{/each}} 
               
               </tbody>
            </table>
         </div> 

      </div>
