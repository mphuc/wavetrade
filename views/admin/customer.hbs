<div class="content">
   <div class="container-fluid">
      <div class="row">
         <div class="col-md-12">
            <p class="pull-right">Total USD: {{formatusd total_balance}} USD </p>  
               
         </div>
         <div class="col-md-12">
         
            <div class="card">

               <div class="content">
                  
                  <div class="material-datatables">
                     <table id="datatables_customer" class="table table-striped table-no-bordered table-hover" style="width:100%;cellspacing:0">
                        <thead>
                           <tr>
                              <th>Date</th>
                              <th>Email</th>
                              <th>Balance</th>
                              <th>IB Comm %</th>
                              <th>Verified</th>
                              <th class="disabled-sorting text-right">Actions</th>
                           </tr>
                        </thead>
                        <tbody>
                           {{#each users}} 
                           <tr>
                              <td>{{formatDate this.signupDate "MM/DD/YYYY LT"}} </td>
                            
                              <td>{{this.email}}</td>
                              
                              <td>{{formatusd this.balance}}</td>
                              <td>
                                 {{#if_eq this.level '0'}}
                                   0%
                                 {{/if_eq}}  
                                 {{#if_eq this.level '1'}}
                                   0.05%
                                 {{/if_eq}}  
                                 {{#if_eq this.level '2'}}
                                   0.2%
                                 {{/if_eq}}  
                                 {{#if_eq this.level '3'}}
                                   0.4%
                                 {{/if_eq}}  
                                 {{#if_eq this.level '4'}}
                                   0.6%
                                 {{/if_eq}}  
                                 {{#if_eq this.level '5'}}
                                   0.8%
                                 {{/if_eq}}  
                                 {{#if_eq this.level '6'}}
                                   0.9 %
                                 {{/if_eq}}  
                              </td>
                              <td>
                                 {{#if_eq this.personal_info.status_doc '0'}}
                                   <span class="label label-default">Not Verified</span>
                                 {{/if_eq}}   
                                 {{#if_eq this.personal_info.status_doc '1'}}
                                    <span class="label label-warning">Watting</span>
                                 {{/if_eq}} 
                                 {{#if_eq this.personal_info.status_doc '2'}}
                                    <span class="label label-success">Finins</span>
                                 {{/if_eq}} 
                              </td>
                              
                              <td class="text-right">
                                 <!-- <a href="#" class="btn btn-simple btn-info btn-icon like"><i class="ti-heart"></i></a> --> <a style="    background: #012a54;" href="edit/customer/{{this._id}}" class="btn btn-simple btn-warning btn-icon edit"><i class="ti-pencil-alt"></i></a> <!-- <a href="#" class="btn btn-simple btn-danger btn-icon remove"><i class="ti-close"></i></a> --> 
                              </td>
                           </tr>
                           {{/each}} 
                        </tbody>
                     </table>
                  </div>
               </div>
            </div>
         </div>
      </div>
   </div>
</div>