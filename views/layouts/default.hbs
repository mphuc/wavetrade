                        <div class="main-wallet">
                           <div class="content">
                              <div class="row">
                                 <div class="col-xs-12">
                                    <div class="">
                                       <p>USD Wallet</p>
                                    </div>
                                 </div>
                              </div>
                           </div>
                           <div class="card-footer"><span class="balance_lending">{{user.balance.lending_wallet.available}}</span> USD</div>
                        </div>
                     </div>
                     <div class="col-md-2">
                        <div class="text-center"><i class="fa fa-arrows-h" style=" font-size: 30px; margin-top: 30px; color: #44c7f4; "></i></div>
                     </div>
                     <div class="col-md-5 ">
                        <div class="main-wallet">
                           <div class="content">
                              <div class="row">
                                 <div class="col-xs-12">
                                    <div class="">
                                       <p>STC Wallet</p>
                                    </div>
                                 </div>
                              </div>
                           </div>
                           <div class="card-footer"><span class="balnace_coin">{{user.balance.coin_wallet.available}} STC</span></div>
                        </div>
                     </div>
                     <div class="form-group">
                        <label class="col-md-12">Amount</label>
                        <div class="col-sm-12">
                           <div class="input-group"><input type="text" placeholder="Amount" class="form-control amount_usd" name="amount" id="amount_transfer"><span class="input-group-addon">USD</span></div>
                        </div>
                     </div>
                     <div class="form-group">
                        <label class="col-md-12">STC</label>
                        <div class="col-sm-12">
                           <div class="input-group"><input type="text" class="form-control amount_coin" readonly="" id="amount_coin_transfer" name="amount_coin"><span class="input-group-addon">STC</span></div>
                        </div>
                     </div>
                  </div>
                  <div class="modal-footer"><button type="button" class="btn btn-social btn-fill btn-twitter" data-dismiss="modal">Close</button><button type="button" class="btn btn-social btn-fill btn-twitter btnConfirm" data-loading-text="<i class='fa fa-spinner fa-spin '></i> Processing Order">Transfer To STC Wallet</button></div>
               </form>
            </div>
         </div>
      </div>
