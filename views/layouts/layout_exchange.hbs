         $.fn.dataTable.ext.search.push(
            function( settings, data, dataIndex ) {
               var min  = $('.History_BuySell_Account_ID #min').val();
               var max  = $('.History_BuySell_Account_ID #max').val();
               var createdAt = data[1] || 0; 
               if  (( min == "" || max == "" ) || ( 
                  moment(createdAt).isSameOrAfter(min+' 00:00 AM') && moment(createdAt).isSameOrBefore(max+' 11:59 PM') )) 
               {
                    return true;
               }
               return false;
            }
        );
