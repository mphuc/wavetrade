                              return intVal(a) + intVal(b);
                          }, 0);

                      $(api.column(4).footer()).html(
                          parseFloat(total).toFixed(2)+' ('+parseFloat(total_all).toFixed(2)+')'
                      );
                      $(api.column(5).footer()).html(
                          parseFloat(totals).toFixed(2) > 0 ? '<span style="color: #2196F3">'+parseFloat(totals).toFixed(2)+' ('+parseFloat(totals_all).toFixed(2)+')</span>' : '<span style="color: red">'+parseFloat(totals).toFixed(2)+' ('+parseFloat(totals_all).toFixed(2)+')</span>'

                      );
                  }
               });
             $('.History_BuySell_Account_ID #min,.History_BuySell_Account_ID #max').change(function() {
