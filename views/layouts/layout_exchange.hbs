         $.fn.dataTable.Api.register( 'sum()', function ( ) {
             return this.flatten().reduce( function ( a, b ) {
                 if ( typeof a === 'string' ) {
                     a = a.replace(/[^\d.-]/g, '') * 1;
                 }
                 if ( typeof b === 'string' ) {
                     b = b.replace(/[^\d.-]/g, '') * 1;
                 }
          
                 return a + b;
             }, 0 );
         } );
         $(document).ready(function() {
             var table_History_BuySell_Account_ID = $('#History_BuySell_Account_ID').DataTable({
                  "order": [[ 1, "desc" ],[ 0, "desc" ]],
                  "footerCallback": function(row, data, start, end, display) {
                      var api = this.api(),
                          data;

                      var intVal = function(i) {
                          return typeof i === 'string' ?
                              i.replace(/[\$,]/g, '') * 1 :
                              typeof i === 'number' ?
                              i : 0;
                      };
                      total = api
                          .column(4, { page: 'current'})
                          .data()
                          .reduce(function(a, b) {
                              return intVal(a) + intVal(b);
                          }, 0);
                      total_all = api
                          .column(4, { })
